{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es5"
    ],
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}